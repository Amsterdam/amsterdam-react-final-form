{
  "name": "@amsterdam/amsterdam-react-final-form",
  "version": "0.1.140",
  "private": false,
  "repository": "https://github.com/amsterdam/amsterdam-react-final-form.git",
  "license": "MIT",
  "description": "Provides adapters for (form-)components defined in [amsterdam-styled-components](https://github.com/Amsterdam/amsterdam-styled-components/pull/491) to be compatible with [react-final-form](https://github.com/final-form/react-final-form).",
  "scripts": {
    "start": "npm run storybook",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public -o docs",
    "lint": "eslint --cache --ext .ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "build": "rm -Rf ./dist && tsc ./src/index.ts --strict --esModuleInterop --declaration --skipLibCheck --jsx react --outDir dist && cp ./package.json ./dist/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "peerDependencies": {
    "@amsterdam/asc-assets": ">= 0.37",
    "@amsterdam/asc-ui": ">= 0.37",
    "final-form": ">= 4",
    "final-form-arrays": ">= 3",
    "react": ">= 16",
    "react-dom": ">= 16",
    "react-final-form": ">= 6",
    "react-final-form-arrays": ">= 3",
    "styled-components": ">= 5"
  },
  "dependencies": {
    "@types/lodash.isempty": "^4.4.7",
    "lodash.chunk": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@amsterdam/asc-assets": "^0.37.0",
    "@amsterdam/asc-ui": "^0.37.0",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/react": "^7.6.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.190",
    "@types/node": "^18.11.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.26",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-plugin-styled-components": "^2.0.7",
    "enzyme": "^3.11.0",
    "eslint": "^7.17.0",
    "final-form": "^4.20.1",
    "final-form-arrays": "^3.0.2",
    "lint-staged": "^13.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-scripts": "^4.0.1",
    "styled-components": "^5.1.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.{ts,tsx}"
    ]
  },
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  }
}
