{
  "name": "@amsterdam/amsterdam-react-final-form",
  "version": "0.1.138",
  "private": false,
  "repository": "https://github.com/amsterdam/amsterdam-react-final-form.git",
  "license": "MIT",
  "description": "Provides adapters for (form-)components defined in [amsterdam-styled-components](https://github.com/Amsterdam/amsterdam-styled-components/pull/491) to be compatible with [react-final-form](https://github.com/final-form/react-final-form).",
  "scripts": {
    "start": "npm run storybook",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public -o docs",
    "lint": "eslint --cache --ext .ts,.tsx src",
    "lint:fix": "npm run lint -- --fix",
    "build": "rm -Rf ./dist && tsc ./src/index.ts --strict --esModuleInterop --declaration --skipLibCheck --jsx react --outDir dist && cp ./package.json ./dist/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "peerDependencies": {
    "@amsterdam/asc-assets": ">= 0.29",
    "@amsterdam/asc-ui": ">= 0.29",
    "final-form": ">= 4",
    "final-form-arrays": ">= 3",
    "react": ">= 16",
    "react-dom": ">= 16",
    "react-final-form": ">= 6",
    "react-final-form-arrays": ">= 3",
    "styled-components": ">= 5"
  },
  "dependencies": {
    "@types/lodash.isempty": "^4.4.6",
    "lodash.chunk": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@amsterdam/asc-assets": "^0.35.0",
    "@amsterdam/asc-ui": "^0.35.2",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.14.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^18.6.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.7",
    "@types/styled-components": "^5.1.10",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "babel-plugin-styled-components": "^2.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.17.0",
    "final-form": "^4.20.1",
    "final-form-arrays": "^3.0.2",
    "lint-staged": "^12.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.8",
    "react-final-form-arrays": "^3.1.2",
    "react-scripts": "^4.0.1",
    "styled-components": "^5.1.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.{ts,tsx}"
    ]
  },
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  }
}
